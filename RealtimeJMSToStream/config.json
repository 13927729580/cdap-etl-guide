{  
  "config":{  
    "instances": 1,
    "stages": [
      {
        "name": "jmsSource",
        "plugin": {  
          "name": "JMS",
          "type": "realtimesource",
          "properties":{  
            "jms.messages.receive": "50",
            "jms.destination.name": "cdap.testtopic",
            "jms.factory.initial": "org.apache.activemq.jndi.ActiveMQInitialContextFactory",
            "jms.provider.url": "tcp://localhost:61616",
            "topic.cdap.testtopic": "cdap.testtopic"
          }
        }
      },
      {  
        "name": "streamSink",
        "plugin": {
          "name": "Stream",
          "type": "realtimesink",
          "properties":{  
            "name": "jmsStream",
            "body.field": "message"
          }
        }
      }
    ],
    "connections": [
      { "from": "jmsSource", "to": "streamSink" }
    ]
  }
}
