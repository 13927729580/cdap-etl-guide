{
  "template": "ETLRealtime",
  "config": {
    "source": {
      "name": "Kafka",
      "properties": {
        "format": "csv",
        "kafka.brokers": "",
        "kafka.partitions": 2,
        "kafka.topic": "myTopic",
        "kafka.zookeeper": "locahost:2181",
        "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"name\",\"type\":\"string\"},{\"name\":\"id\",\"type\":\"int\"}]}"
      }
    },
    "sink": {
      "name": "Table",
      "properties": {
        "name": "output",
        "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"name\",\"type\":\"string\"},{\"name\":\"id\",\"type\":\"int\"}]}",
        "schema.row.field": "id"
      }
    },
    "transforms": []
  }
}
