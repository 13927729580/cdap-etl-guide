{
  "config": {
    "source": {
      "name": "KafkaSource",
      "plugin": {
        "name": "Kafka",
        "properties": {
          "kafka.partitions": "10",
          "kafka.topic": "metrics",
          "kafka.zookeeper": "localhost:2181/cdap/kafka"
        }
      }
    },
    "sinks": [
      {
        "name": "TableSink",
        "plugin": {
          "name": "Table",
          "properties": {
            "name": "metrics",
            "schema.row.field": "message"
          }
        }
      }
    ],
    "connections": [
      { "from": "KafkaSource", "to": "TableSink" }
    ]
  }
}
